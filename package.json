{
  "name": "node-env",
  "version": "0.1.0",
  "description": "",
  "main": "./src/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "npx nodemon --exec npm run babel-node -- ./src/index.js",
    "build": "npx babel ./src --out-dir ./build",
    "babel-node": "npx babel-node --presets='@babel/preset-env' --ignore='foo|bar|baz'",
    "format": "npx prettier --write .",
    "eslint": "npx eslint --fix .",
    "test": "jest",
    "test-menu": "jest --watch"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/eslint-parser": "7.12.1",
    "@babel/node": "7.12.6",
    "@babel/preset-env": "7.12.1",
    "babel-eslint": "10.1.0",
    "babelify": "10.0.0",
    "browser-sync": "2.26.13",
    "browserify": "17.0.0",
    "eslint": "7.13.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-browserify": "0.5.1",
    "gulp-clean": "0.4.0",
    "gulp-clean-css": "4.3.0",
    "gulp-concat": "2.6.1",
    "gulp-rename": "2.0.0",
    "gulp-run": "1.7.1",
    "gulp-watch": "5.0.1",
    "jest": "26.6.3",
    "nodemon": "2.0.6",
    "prettier": "2.1.2"
  },
  "dependencies": {
    "bcrypt": "5.0.0",
    "dotenv": "8.2.0"
  }
